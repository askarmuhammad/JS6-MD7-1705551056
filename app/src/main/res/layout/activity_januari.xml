<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.msi.latihan1.JanuariActivity">

    <TextView
        android:id="@+id/textacara"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="nama acara">

    </TextView>

    <TextView
        android:layout_marginTop="50dp"
        android:id="@+id/texttanggal"
        android:text="tanggal acara"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

    </TextView>

    <ImageView
        android:id="@+id/imagefoto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </ImageView>
    <ImageButton
        android:layout_width="97dp"
        android:layout_height="97dp"
        android:background="@drawable/plus"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:onClick="buttonplus">

    </ImageButton>
</RelativeLayout>
